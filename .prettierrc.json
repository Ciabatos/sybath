{
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 120,
  "bracketSameLine": false,
  "semi": false,
  "singleAttributePerLine": true,
  "singleQuote": false,
  "plugins": ["prettier-plugin-css-order", "prettier-plugin-tailwindcss"],
  "arrowParens": "always",
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": true,
  "proseWrap": "always",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "trailingComma": "all"
}
